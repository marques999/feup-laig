			<DESCENDANTS>
				<DESCENDANT id="myterrain" />
			</DESCENDANTS>
		</NODE>
		<NODE id="earth_main">
			<MATERIAL id="default" />	
			<TEXTURE id="earth_Text" />			
			<TRANSLATION x="5.0" y="25.0" z="-200.0" />	
			<SCALE sx="15.0" sy="15.0" sz="15.0"/>									
			<DESCENDANTS>
				<DESCENDANT id="earth_T" />
			</DESCENDANTS>
		</NODE>
		<NODE id="earth_T">
			<MATERIAL id="default" />	
			<TEXTURE id="earth_Text" />
