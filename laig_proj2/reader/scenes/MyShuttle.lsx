				<ANIMATION id="earth_anim" />				
			</ANIMATIONREF>									
			<DESCENDANTS>
				<DESCENDANT id="earth" />
			</DESCENDANTS>
		</NODE>
		<NODE id="sun_main">
			<MATERIAL id="sun_I" />	
			<TEXTURE id="earth_Text" />			
			<TRANSLATION x="150.0" y="30.0" z="-80.0" />	
			<SCALE sx="1.5" sy="1.5" sz="1.5"/>									
			<DESCENDANTS>
				<DESCENDANT id="sun_T" />
			</DESCENDANTS>
		</NODE>
		<NODE id="sun_T">
			<MATERIAL id="sun_I" />	
			<TEXTURE id="sun_Text" />											
			<DESCENDANTS>
				<DESCENDANT id="earth" />
			</DESCENDANTS>
		</NODE>
		<NODE id="space">
			<MATERIAL id="space_I" />	
			<TEXTURE id="space_Text" />	
			<TRANSLATION x="0.0" y="50.0" z="-70.0" />	
