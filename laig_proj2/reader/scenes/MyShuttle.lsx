		<LEAF id="pau" type="cylinder" args="1.0 1.0 1.0 20 20" />
	</LEAVES>
	<NODES>
		<ROOT id="scene" />
		<NODE id="scene">
			<MATERIAL id="default" />
			<TEXTURE id="colors" />
			<DESCENDANTS>
				<DESCENDANT id="vehicle_mov" />
				<DESCENDANT id="moonSurface" />
				<DESCENDANT id="earth_main" />
				<DESCENDANT id="sun_main" />				
				<DESCENDANT id="space" />
				<DESCENDANT id="flag" />
			</DESCENDANTS>
		</NODE>
		<NODE id="vehicle_mov">
			<MATERIAL id="default" />
			<TEXTURE id="null" />
			<ROTATION axis="z" angle="-90" />
			<DESCENDANTS>
				<DESCENDANT id="vehicle"/>
			</DESCENDANTS>
		</NODE>
		<NODE id="vehicle">
			<MATERIAL id="default" />
			<TEXTURE id="null" />			
			<ANIMATIONREF>
				<ANIMATION id="first" />
				<ANIMATION id="second" />
				<ANIMATION id="third" />
				<ANIMATION id="fourth" />
				<ANIMATION id="fifth" />
				<ANIMATION id="sixth" />
				<ANIMATION id="seventh" />
