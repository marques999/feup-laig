<SCENE>
	<INITIALS>
		<frustum near="0.1" far="500" />	
		<translation x="-25.0" y="-40.0" z="-45.0" />		
		<rotation axis="y" angle="60.0" />	
		<rotation axis="x" angle="-35.0" />
		<rotation axis="z" angle="-10.0" />						
		<scale sx="1.0" sy="1.0" sz="1.0" />
		<reference length="4.0" />
	</INITIALS>
	<ILLUMINATION>
		<ambient r="0.1" g="0.1" b="0.1" a="1.0" />
		<background r="0.08" g="0.08" b="0.12" a="1.0" />
	</ILLUMINATION>
	<LIGHTS>	
		<LIGHT id="luz1">
			<enable value="1" />
			<position x="260.0" y="0.0" z="260.0" w="1.0" />
			<ambient r="0.0" g="0.0" b="0.0" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.8" a="0.8" />
			<specular r="0.9" g="0.9" b="0.9" a="1.0" />
		</LIGHT>	
		<LIGHT id="luz2">
			<enable value="1" />
			<position x="260.0" y="0.0" z="-260.0" w="1.0" />
			<ambient r="0.0" g="0.0" b="0.0" a="1.0" />
			<diffuse r="0.8" g="0.8" b="0.8" a="0.8" />
			<specular r="0.9" g="0.9" b="0.9" a="1.0" />
		</LIGHT>		
	<TEXTURES>
		<TEXTURE id="colors">
			<file path="images/colors.jpg" />
			<amplif_factor s="1.0" t="1.0" />
		</TEXTURE>
		<TEXTURE id="earth_Text">
			<file path="images/earth.jpg" />
			<amplif_factor s="1.0" t="1.0" />
		</TEXTURE>
		<TEXTURE id="sun_Text">
			<file path="images/sun.jpg" />
			<amplif_factor s="1.0" t="1.0" />
		</TEXTURE>
		<TEXTURE id="space_Text">
			<file path="images/scape.jpg" />
			<amplif_factor s="1.0" t="1.0" />
		</TEXTURE>
	</TEXTURES>
	<MATERIALS>
		<MATERIAL id="default">
			<shininess value="5" />
			<specular r="0.5" g="0.5" b="0.5" a="1.0" />
			<diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
			<ambient r="0.5" g="0.5" b="0.5" a="1.0" />
			<emission r="0.0" g="0.0" b="0.0" a="0.0" />
		</MATERIAL>
		<MATERIAL id="space_I">
			<shininess value="5" />
			<specular r="0.5" g="0.5" b="0.5" a="1.0" />
			<diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
			<ambient r="0.5" g="0.5" b="0.5" a="1.0" />
			<emission r="0.7" g="0.7" b="0.7" a="0.7" />
		</MATERIAL>
		<MATERIAL id="sun_I">
			<shininess value="5" />
			<specular r="0.5" g="0.5" b="0.5" a="1.0" />
			<diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
			<ambient r="0.5" g="0.5" b="0.5" a="1.0" />
			<emission r="0.7" g="0.7" b="0.7" a="0.7" />
		</MATERIAL>
	</MATERIALS>
	<ANIMATIONS>
		<ANIMATION id="earth_anim" span="60.0" type="circular" center="0 0 0" radius="0" startang="0" rotang="360" >		
		</ANIMATION>
		<ANIMATION id="first" span="0.5" type="linear">
			<controlpoint x="0.0" y="0.0" z="0.0" />
			<controlpoint x="-0.5" y="0.0" z="0.0" />
		</ANIMATION>
		<ANIMATION id="second" span="0.7" type="linear">
			<controlpoint x="-0.5" y="0.0" z="0.0" />
