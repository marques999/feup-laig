				<DESCENDANT id="earth" />
			</DESCENDANTS>
		</NODE>
		<NODE id="flag">
			<MATERIAL id="null" />	
			<TEXTURE id="null" />	
			<TRANSLATION x="-35.0" y="3.0" z="54.0" />				
			<SCALE sx="0.7" sy="0.7" sz="0.7"/>	
			<ROTATION axis="z" angle="-180" />	
			<ROTATION axis="y" angle="-40" />										
			<DESCENDANTS>
				<DESCENDANT id="stick" />
				<DESCENDANT id="pano" />				
			</DESCENDANTS>
		</NODE>
		<NODE id="pano">
			<MATERIAL id="null" />	
			<TEXTURE id="null" />	
			<SCALE sx="0.5" sy="0.4" sz="0.5"/>
			<ROTATION axis="y" angle="-90" />										
			<DESCENDANTS>
				<DESCENDANT id="bandeira" />				
			</DESCENDANTS>
		</NODE>
		<NODE id="stick">
			<MATERIAL id="null" />	
			<TEXTURE id="null" />	
			<TRANSLATION x="0.0" y="-0.1" z="-0.1" />	
			<SCALE sx="0.08" sy="3.0" sz="0.08"/>		
			<ROTATION axis="x" angle="-90" />								
			<DESCENDANTS>
				<DESCENDANT id="pau" />				
			</DESCENDANTS>
		</NODE>
	</NODES>
</SCENE>