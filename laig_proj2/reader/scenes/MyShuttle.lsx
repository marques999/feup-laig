		<LEAF id="myV" type="vehicle"/>
		<LEAF id="myterrain" type="terrain" texture="scenes/images/moon.jpg" heightmap="scenes/images/moon.jpg" />
		<LEAF id="earth" type="sphere" args="1.0 20 20" />
		<LEAF id="bandeira" type="patch" partsU="20" partsV="20" orderU="3" orderV="3">
			<controlpoint coords="0.0 0.0 0.0 1.0"/>
			<controlpoint coords="2.0 0.0 1.0 1.0"/>
			<controlpoint coords="0.0 0.0 2.0 1.0"/>
			<controlpoint coords="2.0 0.0 3.0 1.0"/>

